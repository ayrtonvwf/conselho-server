{
    "require": {
        "php": "^7.2",
        "ext-sodium": "*",
        "ext-PDO": "*",
        "ext-json": "*",
        "vlucas/phpdotenv": "^2.5",
        "miladrahimi/phprouter": "2.*",
        "vlucas/valitron": "^1.4",
        "atlas/orm": "2.*",
        "sentry/sentry": "^1.9",
        "codeguy/upload": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Conselho\\": "Conselho/"
        }
    },
    "require-dev": {
        "atlas/cli": "1.*"
    },
    "scripts": {
        "serve": "php -S localhost:8000"
    },
    "config": {
        "optimize-autoloader": true,
        "process-timeout": 0
    }
}
